    "import numpy as np\n",
    "\n",
    "# Dot product\n",
    "x = np.array([[1, 2], [3, 4]])\n",
    "y = x.copy()\n",
    "m = np.asmatrix(x)\n",
    "n = np.asmatrix(y)\n",
    "x[0, 0] = 5\n",
    "\n",
    "print(m)\n",
    "print(n)\n",
    "\n",
    "print(m * n)\n"
   ],
